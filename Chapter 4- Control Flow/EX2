alien_color = 'green'

if alien_color == 'green':
    print("Congratulations! You just earned 5 points for shooting the green alien.")
else:
    print("Congratulations! You just earned 10 points for shooting an alien that isn't green.")

alien_color = 'red'

if alien_color == 'green':
    print("Congratulations! You just earned 5 points for shooting the green alien.")
else:
    print("Congratulations! You just earned 10 points for shooting an alien that isn't green.")